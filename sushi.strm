stream sushiswapTest;

interface Sushi {
    event PoolCreated(
        address indexed token0,
        address indexed token1,
        uint24 indexed fee,
        int24 tickSpacing,
        address pool
    );
}

struct Pools {
     address token0;
     address token1;
}

map pools_created:
(Sushi.PoolCreated[]) -> Pools[] {
    map (p) => Pools {
        token0: p.token0,
        token1: p.token1,
    };
}
